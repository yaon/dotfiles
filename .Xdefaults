/* xconsole  */
xconsole*font:                  7x12bold
xconsole*background:            black
xconsole*foreground:            white
xconsole*geometry:              +0-0

/* ns_xm */
Xm*font:                        7x13bold
Xm*foreground:                  white
Xm*background:                  black
Xm*geometry:                    400x200+0-0

/* xterm */
/* xrdb -load .Xdefaults */
!xterm*font:                     12x14bold
xterm*faceName: xft:Courrier:   pixelsize=14
xterm*boldmode:                 true
xterm*saveLines:                1000
xterm*scrollBar:                false

! --- special colors ---

xterm*background: #0d1926
xterm*foreground: #d9e6f2

! --- standard colors ---

! black
xterm*color0: #000000

! bright_black
xterm*color8: #262626

! red
xterm*color1: #b87a7a

! bright_red
xterm*color9: #dbbdbd

! green
xterm*color2: #7ab87a

! bright_green
xterm*color10: #bddbbd

! yellow
xterm*color3: #b8b87a

! bright_yellow
xterm*color11: #dbdbbd

! blue
xterm*color4: #7a7ab8

! bright_blue
xterm*color12: #bdbddb

! magenta
xterm*color5: #b87ab8

! bright_magenta
xterm*color13: #dbbddb

! cyan)
xterm*color6: #7ab8b8

! bright_cyan
xterm*color14: #bddbdb

! white
xterm*color7: #d9d9d9

! bright_white
xterm*color15: #ffffff
!term*color15: #000000


!

